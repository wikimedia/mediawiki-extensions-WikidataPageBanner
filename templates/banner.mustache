<div class="ext-wpb-pagebanner noprint">
	<div class="wpb-topbanner">
		<div class="wpb-name">{{title}}</div>
		<a class="image" title="{{tooltip}}" href="{{bannerfile}}"><img src="{{banner}}" srcset="{{srcset}}" class="wpb-banner-image {{originx}}" data-pos-x="{{data-pos-x}}" data-pos-y="{{data-pos-y}}"></a>
		{{#hasIcons}}
		<div class="wpb-iconbox">
			{{#icons}}
				<a href="{{iconurl}}">{{{icon}}}</a>
			{{/icons}}
		</div>
		{{/hasIcons}}
		<div class="wpb-topbanner-toc {{#bottomtoc}}wpb-bottomtoc{{/bottomtoc}}"><div class="wpb-banner-toc">{{{toc}}}</div></div>
	</div>
</div>
