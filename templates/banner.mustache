<div class="noprint">
	<div class="ext-wpb-pagebanner">
		<div class="wpb-topbanner">
			<div class="wpb-name">{{title}}</div>
			<a title="{{tooltip}}" href={{bannerfile}}><img src={{banner}} srcset="{{srcset}}" class="wpb-banner-image"></a>
			{{#hasIcons}}
			<div class="wpb-iconbox">
				{{#icons}}
					{{{icon}}}
				{{/icons}}
			</div>
			{{/hasIcons}}
			<div class="wpb-topbanner-toc"><div class="wpb-banner-toc">{{{toc}}}</div></div>
		</div>
	</div>
</div>
